//1. No input, no return.
Function factorial()
     Variable a = 'Hello',b = 'World'
     BeginFunction
     display 'Hello World'+ a + b
     EndFunction

factorial()

//2. With input, with return
Function Integer factorial(input as Integer)
     Variable a = 1, b = 2
     BeginFunction
     c = a + b + input
     EndFunction
     return c
d = factorial(3)
display d

//3.With input, without return
Function Integer factorial(input as String)
     Variable a = 'Hello', b = "World"
     BeginFunction
     c = a + b + input
     display c
     EndFunction
factorial('!!!')